model Assistant {
  id          String             @id @default(uuid())
  name        String
  version     String
  description String?
  status      String
  createdAt   DateTime           @default(now())
  updatedAt   DateTime           @updatedAt
  metadata    AssistantMetadata? @relation(fields: [metadataId], references: [id])
  metadataId  String?
}
